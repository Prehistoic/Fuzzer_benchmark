%%% CORRECT_STRING

%const% MAX_REPEAT_POWER := 3
%const% VARIANCE_MAX := 1

%section% := value

correct_string :=
	+upChar+%repeat%(+lowChar+)

upChar :=
  %range%(A-Z)

lowChar :=
  %range%(a-z)

%%%===============================================

%section% := variance

main :=
  +correct_string+
